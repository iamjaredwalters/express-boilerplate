{
  "restartable": "rs",
  "delay": "500",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "*.test.js"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --require babel-register --require dotenv/config --inspect"
  },
  "watch": [
    "src/"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}
